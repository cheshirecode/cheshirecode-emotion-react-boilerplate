{
  "trailingComma" : "all",
  "singleQuote": true,
  "printWidth": 120,
  "parser": "typescript",
  "tabWidth": 2,
  "overrides": [
    {
      "files": ["*.js", "*.cjs", "*.mjs"],
      "options": {
        "parser": "javascript"
      }
    },
    {
      "files": ["*.json", "*.jsonc", ".babelrc", ".prettierrc"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
